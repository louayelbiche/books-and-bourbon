      "types": "./dist/circuit-breaker.d.ts",
      "import": "./dist/circuit-breaker.js",
      "default": "./dist/circuit-breaker.js"
    },
    "./correlation": {
      "types": "./dist/correlation.d.ts",
      "import": "./dist/correlation.js",
      "default": "./dist/correlation.js"
    },
    "./retry": {
      "types": "./dist/retry.d.ts",
